{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		// "semi": "off",
		"arrow-parens": "off",
		"comma-dangle": "off",
		"max-len": "off",
		"linebreak-style": "off",
		"no-unused-vars": "off",
		"indent": "off",
		"no-tabs": "off",
		"no-mixed-spaces-and-tabs": "off",
		"eol-last": "off",
		"require-jsdoc": "off"
	},
		"extends": ["eslint:recommended", "google"]
}